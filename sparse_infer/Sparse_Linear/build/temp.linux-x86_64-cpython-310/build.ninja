ninja_required_version = 1.3
cxx = /usr/bin/g++-11
nvcc = /usr/local/cuda-12.1/bin/nvcc

cflags = -march=nocona -mtune=haswell -ftree-vectorize -fPIC -fstack-protector-strong -fno-plt -O2 -ffunction-sections -pipe -isystem /home/zjnyly/miniconda3/envs/rheo/include -DNDEBUG -D_FORTIFY_SOURCE=2 -O2 -isystem /home/zjnyly/miniconda3/envs/rheo/include -fPIC -I/home/zjnyly/miniconda3/envs/rheo/lib/python3.10/site-packages/torch/include -I/home/zjnyly/miniconda3/envs/rheo/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/home/zjnyly/miniconda3/envs/rheo/lib/python3.10/site-packages/torch/include/TH -I/home/zjnyly/miniconda3/envs/rheo/lib/python3.10/site-packages/torch/include/THC -I/usr/local/cuda-12.1/include -I/home/zjnyly/miniconda3/envs/rheo/include/python3.10 -c
post_cflags = -O3 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=sparse_linear -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -I/home/zjnyly/miniconda3/envs/rheo/lib/python3.10/site-packages/torch/include -I/home/zjnyly/miniconda3/envs/rheo/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/home/zjnyly/miniconda3/envs/rheo/lib/python3.10/site-packages/torch/include/TH -I/home/zjnyly/miniconda3/envs/rheo/lib/python3.10/site-packages/torch/include/THC -I/usr/local/cuda-12.1/include -I/home/zjnyly/miniconda3/envs/rheo/include/python3.10 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O3 -lineinfo -arch=sm_75 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=sparse_linear -D_GLIBCXX_USE_CXX11_ABI=0 -ccbin /usr/bin/gcc-11 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





